end
end
TweenService:Create(tabButton, TweenInfo.new(0.15, Enum.EasingStyle.Quad), {BackgroundTransparency = 0.82, TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
local tabName = tabButton.Name:gsub(â€œTabâ€, â€œâ€)
HomeSection.Visible = false; TargetSection.Visible = false; AnimationSection.Visible = false
CombatSection.Visible = false; MiscSection.Visible = false; VisualSection.Visible = false
if tabName == â€œHomeâ€ then HomeSection.Visible = true
elseif tabName == â€œPlayerâ€ then TargetSection.Visible = true
elseif tabName == â€œAnimationâ€ then AnimationSection.Visible = true
elseif tabName == â€œCombatâ€ then CombatSection.Visible = true
elseif tabName == â€œVisualâ€ then VisualSection.Visible = true
elseif tabName == â€œMiscâ€ then MiscSection.Visible = true
end
end)
end

