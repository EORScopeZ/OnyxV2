â€“ Face Bang logic

local FaceBangEnabled = false
local faceBangThread = nil
local faceBangTarget = nil  â€“ the player weâ€™re attached to
local faceBangPhase = 1     â€“ 1 = forward, -1 = backward

local function GetNearestPlayer()
local char = plr.Character
if not char then return nil end
local hrp = char:FindFirstChild(â€œHumanoidRootPartâ€)
if not hrp then return nil end

```
local nearest = nil
local nearestDist = math.huge

for _, p in pairs(Players:GetPlayers()) do
    if p ~= plr and p.Character then
        local otherHRP = p.Character:FindFirstChild("HumanoidRootPart")
        if otherHRP then
            local dist = (hrp.Position - otherHRP.Position).Magnitude
            if dist < nearestDist then
                nearestDist = dist
                nearest = p
            end
        end
    end
end
return nearest
```

end

local function StopFaceBang()
FaceBangEnabled = false
faceBangTarget = nil
if faceBangThread then
task.cancel(faceBangThread)
faceBangThread = nil
end
â€“ Unfreeze
pcall(function()
local char = plr.Character
if not char then return end
local hum = char:FindFirstChildOfClass(â€œHumanoidâ€)
if hum then
hum.PlatformStand = false
hum.AutoRotate = true
hum.Sit = false
end
local hrp = char:FindFirstChild(â€œHumanoidRootPartâ€)
if hrp then
hrp.AssemblyLinearVelocity = Vector3.new(0,0,0)
hrp.AssemblyAngularVelocity = Vector3.new(0,0,0)
end
end)
pcall(function()
if FBToggleBtn and FBToggleBtn.Parent then
FBToggleBtn.Text = â€œâ–¶ Start (Z)â€
FBToggleBtn.BackgroundTransparency = 0.88
end
if FBStatusLabel and FBStatusLabel.Parent then
FBStatusLabel.Text = â€œStatus: Idle | Press Z to attach to nearestâ€
end
end)
SendNotify(â€œFace Bangâ€, â€œStoppedâ€, 2)
end

local function StartFaceBang(targetPlayer)
if FaceBangEnabled then StopFaceBang() end

```
local char = plr.Character
if not char then SendNotify("Error", "No character", 2) return end
local hum = char:FindFirstChildOfClass("Humanoid")
local hrp = char:FindFirstChild("HumanoidRootPart")
if not hum or not hrp then return end

FaceBangEnabled = true
faceBangTarget = targetPlayer
faceBangPhase = 1

-- Freeze local character
hum.PlatformStand = true
hum.AutoRotate = false
hum.Sit = true

pcall(function()
    if FBToggleBtn and FBToggleBtn.Parent then
        FBToggleBtn.Text = "â–  Stop (Z)"
        FBToggleBtn.BackgroundTransparency = 0.6
    end
    if FBStatusLabel and FBStatusLabel.Parent then
        FBStatusLabel.Text = "Status: Active â†’ " .. targetPlayer.DisplayName
    end
end)

faceBangThread = task.spawn(function()
    local oscillatorTime = 0

    while FaceBangEnabled do
        RunService.Heartbeat:Wait()

        local localChar = plr.Character
        if not localChar then break end
        local localHRP = localChar:FindFirstChild("HumanoidRootPart")
        local localHum = localChar:FindFirstChildOfClass("Humanoid")
        if not localHRP or not localHum then
            task.wait(0.1)
            continue
        end

        local targetChar = faceBangTarget and faceBangTarget.Character
        if not targetChar then
            pcall(function() if FBStatusLabel and FBStatusLabel.Parent then FBStatusLabel.Text = "Status: Waiting for target..." end end)
            task.wait(0.3)
            continue
        end

        local targetHead = targetChar:FindFirstChild("Head")
        local targetHRP = targetChar:FindFirstChild("HumanoidRootPart")
        if not targetHead or not targetHRP then
            task.wait(0.1)
            continue
        end

        -- Keep completely upright and frozen - NO Sit (causes knee bend), NO look-at (causes tilt)
        localHum.PlatformStand = true
        localHum.AutoRotate = false
        localHum.Sit = false

        -- Stiffen the target player every frame
        pcall(function()
            local targetHum = targetChar:FindFirstChildOfClass("Humanoid")
            if targetHum then
                targetHum.PlatformStand = true
                targetHum.AutoRotate = false
            end
            targetHRP.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
            targetHRP.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
        end)

        local speed = getFBSpeed()
        local distance = getFBDistance()

        -- Fast oscillation
        oscillatorTime = oscillatorTime + (speed / 3) * 0.016
        local oscillation = math.sin(oscillatorTime) -- -1 to 1

        local headCF = targetHead.CFrame
        local forward = headCF.LookVector

        -- Face position: 0.5 studs in front of head surface
        local facePos = targetHead.Position + (forward * 0.5) + Vector3.new(0, 1, 0)

        -- Oscillate 0..distance in front of face only
        local t = (oscillation + 1) / 2
        local finalPos = facePos + (forward * t * distance)

        -- yaw only, stay upright
        local targetYaw = math.atan2(-headCF.LookVector.Z, headCF.LookVector.X) - math.pi/2 + math.pi
        local uprightCF = CFrame.new(finalPos) * CFrame.Angles(0, targetYaw, 0)

        local dist = (localHRP.Position - finalPos).Magnitude
        if dist > 10 then
            localHRP.CFrame = uprightCF
        else
            local newPos = localHRP.Position:Lerp(finalPos, 0.9)
            localHRP.CFrame = CFrame.new(newPos) * CFrame.Angles(0, targetYaw, 0)
        end

        localHRP.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        localHRP.AssemblyAngularVelocity = Vector3.new(0, 0, 0)

        if canUsePhysicsRep then
            pcall(function()
                sethiddenproperty(localHRP, "PhysicsRepRootPart", targetHead)
            end)
        end
    end

    StopFaceBang()
end)

SendNotify("Face Bang", "Attached to " .. targetPlayer.DisplayName, 3)
```

end

FBToggleBtn.MouseButton1Click:Connect(function()
