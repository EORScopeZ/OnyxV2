â€“ KEY SYSTEM - AUTHENTICATION
local KEY_SERVER_URL = â€œhttps://onyx-bot-production.up.railway.appâ€

local function buildUrl(endpoint)
return KEY_SERVER_URL:gsub(â€/$â€, â€œâ€) .. â€œ/â€ .. endpoint:gsub(â€^/â€, â€œâ€)
end
local function httpRequest(options)
if request then
return request(options)
elseif http and http.request then
return http.request(options)
elseif http_request then
return http_request(options)
elseif syn and syn.request then
return syn.request(options)
elseif fluxus and fluxus.request then
return fluxus.request(options)
else
return HttpService:RequestAsync(options)
end
end

local function ValidateKey(key)
local robloxUser = plr.Name
local ok, result = pcall(function()
return httpRequest({
Url    = buildUrl(â€/validateâ€),
Method = â€œPOSTâ€,
Headers = { [â€œContent-Typeâ€] = â€œapplication/jsonâ€ },
Body   = HttpService:JSONEncode({ key = key, roblox_user = robloxUser })
})
end)
if not ok or not result then
warn(â€[Onyx] HTTP failed: â€œ .. tostring(result))
return { valid = false, message = â€œCould not reach key server.â€ }
end
local ok2, decoded = pcall(HttpService.JSONDecode, HttpService, result.Body)
if not ok2 then
warn(â€[Onyx] Bad response body: â€œ .. tostring(result.Body))
return { valid = false, message = â€œInvalid response from key server.â€ }
end
return decoded
end

