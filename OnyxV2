        for i = 1, 10 do
            teleportEffect.Transparency = teleportEffect.Transparency + 0.05
            teleportEffect.Size = teleportEffect.Size + Vector3.new(0.5, 0, 0.5)
            teleportEffect.CFrame = CFrame.new(mouseLocation)
            task.wait(0.05)
        end
        teleportEffect:Destroy()
